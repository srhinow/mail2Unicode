-- 
-- Table `tl_module`
-- 

CREATE TABLE `tl_module` (
 `cnt_template` varchar(255) NOT NULL default '', 
 `cnt_jumpTo` smallint(5) unsigned NOT NULL default '0',
 `cnt_boxtext` varchar(255) NOT NULL default '',
 `cnt_api_url` varchar(255) NOT NULL default '',
 `cnt_api_key` varchar(55) NOT NULL default '',
 `cnt_api_project` varchar(255) NOT NULL default '',
 `cnt_api_campaign` varchar(255) NOT NULL default '',
) ENGINE=MyISAM DEFAULT CHARSET=utf8;
